{
    "name": "glints-test",
    "version": "1.0.0",
    "main": "dist/index.js",
    "license": "MIT",
    "scripts": {
        "build": "yarn tsc",
        "tsc": "tsc --project tsconfig.compile.json",
        "tsc:w": "tsc --project tsconfig.json -w",
        "start": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node tsconfig-paths/register src/index.ts --env=development",
        "start:prod": "cross-env NODE_ENV=production node -r ./tsconfig-paths-bootstrap.js dist/index.js",
        "test": "yarn test:lint && yarn test:coverage",
        "test:unit": "cross-env NODE_ENV=test mocha",
        "test:coverage": "cross-env NODE_ENV=test nyc mocha",
        "test:lint": "tslint --project tsconfig.json",
        "test:lint:fix": "tslint --project tsconfig.json --fix",
        "prettier": "prettier '{src,test}/**/*.ts' --write",
        "serve": "nodemon --config nodemon.json",
        "seed": "npx ts-node src/seeder",
        "migrate": "npx sequelize-cli db:migrate"
    },
    "dependencies": {
        "@tsed/ajv": "5.49.0",
        "@tsed/common": "5.49.0",
        "@tsed/core": "5.49.0",
        "@tsed/di": "5.49.0",
        "body-parser": "1.19.0",
        "compression": "^1.7.4",
        "cookie-parser": "1.4.5",
        "cors": "2.8.5",
        "cross-env": "7.0.2",
        "dotenv": "^8.2.0",
        "express": "4.17.1",
        "express-session": "^1.17.1",
        "express-useragent": "^1.0.15",
        "method-override": "^3.0.0",
        "moment": "^2.25.3",
        "mysql2": "^2.1.0",
        "reflect-metadata": "^0.1.13",
        "sequelize": "^5.21.7",
        "sequelize-typescript": "^1.1.0",
        "tsconfig-paths": "^3.9.0"
    },
    "devDependencies": {
        "@tsed/cli-plugin-mocha": "1.10.2",
        "@tsed/cli-plugin-mongoose": "1.10.2",
        "@tsed/cli-plugin-passport": "1.10.2",
        "@tsed/cli-plugin-tslint": "1.10.2",
        "@tsed/testing": "5.49.0",
        "@types/bcrypt": "^3.0.0",
        "@types/bluebird": "^3.5.30",
        "@types/chai": "^4.2.11",
        "@types/chai-as-promised": "7.1.2",
        "@types/command-line-args": "^5.0.0",
        "@types/compression": "1.7.0",
        "@types/cookie-parser": "1.4.2",
        "@types/cors": "2.8.6",
        "@types/cryptr": "^4.0.0",
        "@types/express": "4.17.6",
        "@types/express-session": "^1.17.0",
        "@types/express-useragent": "^1.0.0",
        "@types/method-override": "0.0.31",
        "@types/mocha": "^7.0.2",
        "@types/module-alias": "^2.0.0",
        "@types/multer": "^1.4.3",
        "@types/node": "13.13.4",
        "@types/sinon": "9.0.0",
        "@types/sinon-chai": "3.2.4",
        "@types/socket.io": "^2.1.4",
        "@types/supertest": "2.0.8",
        "@types/swagger-schema-official": "2.0.20",
        "@types/underscore": "^1.10.0",
        "@types/validator": "^13.0.0",
        "chai": "^4.2.0",
        "chai-as-promised": "7.1.1",
        "concurrently": "5.2.0",
        "eslint": "^7.0.0",
        "husky": "4.2.5",
        "lint-staged": "10.1.7",
        "mocha": "^7.1.2",
        "nodemon": "2.0.3",
        "nyc": "15.0.1",
        "prettier": "2.0.5",
        "random-email": "^1.0.3",
        "sequelize-cli": "^6.2.0",
        "sinon": "9.0.2",
        "sinon-chai": "3.5.0",
        "source-map-support": "^0.5.19",
        "supertest": "4.0.2",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.2"
    }
}
